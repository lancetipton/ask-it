{
  "name": "askit",
  "version": "0.0.1",
  "description": "Node CLI helper for asking questions",
  "main": "src/index.js",
  "repository": "https://github.com/lancetipton/askIt",
  "author": "Lance Tipton",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">= 12.15.0"
  },
  "bin": {
    "askIt": "./src/askIt"
  },
  "files": [
    "src/"
  ],
  "scripts": {
    "validate": "node ./scripts/validate.js",
    "test": "NODE_ENV=test jest",
    "test:reports": "yarn test --coverage && yarn view:reports"
  },
  "preferGlobal": true,
  "dependencies": {
    "@ltipton/jsutils": "^5.0.0",
    "colors": "1.4.0",
    "inquirer": "7.0.6",
    "module-alias": "2.2.2"
  },
  "devDependencies": {
    "jest": "24.8.0",
    "jest-cli": "24.8.0",
    "jest-html-reporter": "3.1.3",
    "jest-sonar-reporter": "2.0.0"
  },
  "_moduleAliases": {
    "KegAsk": "src/askIt",
    "KegLog": "src/logger",
    "KegMocks": "src/__mocks__",
    "KegModels": "src/models",
    "KegPrompts": "src/prompts",
    "KegRoot": ".",
    "KegSrc": "src"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "globals": {
      "__DEV__": true
    },
    "moduleNameMapper": {
      "^KegAsk(.*)$": "<rootDir>/src/askIt$1",
      "^KegLog(.*)$": "<rootDir>/src/logger$1",
      "^KegMocks(.*)$": "<rootDir>/src/__mocks__$1",
      "^KegModels(.*)$": "<rootDir>/src/models$1",
      "^KegPrompts(.*)$": "<rootDir>/src/prompts$1",
      "^KegRoot(.*)$": "<rootDir>$1",
      "^KegSrc(.*)$": "<rootDir>/src$1"
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)"
    ],
    "transformIgnorePatterns": [
      ".*"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js}",
      "!**/__mocks__/**/*.{js}"
    ],
    "coverageDirectory": "reports/coverage",
    "setupFilesAfterEnv": [
      "<rootDir>/scripts/setupTests.js"
    ]
  }
}
